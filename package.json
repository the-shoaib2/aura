{
	"name": "aura-monorepo",
	"version": "1.0.0",
	"private": true,
	"engines": {
		"node": ">=22.16",
		"pnpm": ">=10.18.3"
	},
	"packageManager": "pnpm@10.18.3",
	"scripts": {
		"prepare": "node scripts/prepare.mjs || echo \"Script not found\"",
		"preinstall": "node scripts/block-npm-install.js || echo \"Script not found\"",
		"build": "turbo run build",
		"typecheck": "turbo typecheck",
		"dev": "turbo run dev --parallel --env-mode=loose",
		"dev:be": "turbo run dev --parallel --env-mode=loose --filter=!apps/web --filter=!apps/docs",
		"dev:fe": "turbo run dev --parallel --env-mode=loose --filter=apps/web --filter=apps/docs",
		"clean": "turbo run clean",
		"reset": "node scripts/ensure-zx.mjs && zx scripts/reset.mjs || echo \"Script not found\"",
		"format": "turbo run format && node scripts/format.mjs || echo \"Script not found\"",
		"format:check": "turbo run format:check",
		"lint": "turbo run lint",
		"lint:affected": "turbo run lint --affected",
		"lint:fix": "turbo run lint:fix",
		"lint:styles": "turbo run lint:styles",
		"lint:styles:fix": "turbo run lint:styles:fix",
		"test": "JEST_JUNIT_CLASSNAME={filepath} turbo run test",
		"test:ci": "turbo run test --continue --concurrency=1",
		"test:ci:frontend": "turbo run test --continue --filter='./apps/**'",
		"test:ci:backend": "turbo run test --continue --concurrency=1 --filter='!./apps/**'",
		"test:ci:backend:unit": "turbo run test:unit --continue --filter='!./apps/**'",
		"test:ci:backend:integration": "turbo run test:integration --continue --concurrency=1 --filter='!./apps/**'",
		"test:unit": "turbo run test:unit",
		"test:integration": "turbo run test:integration",
		"test:affected": "turbo run test --affected --concurrency=1",
		"watch": "turbo run watch --concurrency=30"
	},
	"devDependencies": {
		"@aura/eslint-config": "workspace:*",
		"@biomejs/biome": "^1.9.0",
		"@types/jest": "^29.5.3",
		"@types/node": "*",
		"@types/supertest": "^6.0.3",
		"babel-plugin-transform-import-meta": "^2.3.2",
		"bundlemon": "^3.1.0",
		"cross-env": "^7.0.3",
		"eslint": "catalog:",
		"get-tsconfig": "^4.10.0",
		"husky": "^8.0.0",
		"jest": "^29.6.2",
		"jest-environment-jsdom": "^29.6.2",
		"jest-expect-message": "^1.1.3",
		"jest-junit": "^16.0.0",
		"jest-mock": "^29.6.2",
		"jest-mock-extended": "^3.0.4",
		"lefthook": "^1.7.15",
		"license-checker": "^25.0.1",
		"nock": "^14.0.1",
		"nodemon": "^3.0.1",
		"npm-run-all2": "^7.0.2",
		"p-limit": "^3.1.0",
		"prettier": "^3.6.2",
		"rimraf": "^5.0.1",
		"run-script-os": "^1.0.7",
		"supertest": "^7.1.1",
		"ts-jest": "^29.1.1",
		"tsc-alias": "^1.8.10",
		"tsc-watch": "^6.2.0",
		"turbo": "2.5.4",
		"typescript": "catalog:",
		"vitest": "catalog:",
		"vitest-mock-extended": "catalog:",
		"zx": "^8.1.4"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"sqlite3"
		],
		"overrides": {
			"@types/node": "^20.17.50",
			"axios": "catalog:",
			"chokidar": "catalog:",
			"esbuild": "^0.25.0",
			"form-data": "catalog:",
			"semver": "^7.5.4",
			"tslib": "^2.6.2",
			"tsconfig-paths": "^4.2.0",
			"typescript": "catalog:",
			"ws": ">=8.17.1",
			"nodemailer": "catalog:",
			"validator": "13.15.20"
		},
		"patchedDependencies": {}
	}
}
