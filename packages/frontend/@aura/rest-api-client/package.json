{
  "name": "@aura/rest-api-client",
  "type": "module",
  "version": "1.22.0",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    }
  },
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest run",
    "test:dev": "vitest --silent=false",
    "lint": "eslint src --quiet",
    "lint:fix": "eslint src --fix",
    "format": "biome format --write . && prettier --write . --ignore-path ../../../../.prettierignore",
    "format:check": "biome ci . && prettier --check . --ignore-path ../../../../.prettierignore"
  },
  "dependencies": {
    "@aura/api-types": "workspace:*",
    "@aura/constants": "workspace:*",
    "@aura/permissions": "workspace:*",
    "@aura/utils": "workspace:*",
    "js-base64": "catalog:",
    "workflow": "workspace:*",
    "axios": "catalog:",
    "flatted": "catalog:"
  },
  "devDependencies": {
    "@aura/eslint-config": "workspace:*",
    "@aura/i18n": "workspace:*",
    "@aura/typescript-config": "workspace:*",
    "@aura/vitest-config": "workspace:*",
    "@testing-library/jest-dom": "catalog:frontend",
    "@testing-library/user-event": "catalog:frontend",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  },
  "license": "See LICENSE.md file in the root of the repository"
}
