{
  "name": "@aura/api-types",
  "version": "0.53.0",
  "scripts": {
    "clean": "rimraf dist .turbo",
    "dev": "pnpm watch",
    "typecheck": "tsc --noEmit",
    "build": "tsc -p tsconfig.build.json",
    "format": "biome format --write .",
    "format:check": "biome ci .",
    "lint": "eslint . --quiet",
    "lint:fix": "eslint . --fix",
    "watch": "tsc -p tsconfig.build.json --watch",
    "test": "jest",
    "test:dev": "jest --watch"
  },
  "main": "dist/index.js",
  "module": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@aura/typescript-config": "workspace:*",
    "@aura/config": "workspace:*"
  },
  "dependencies": {
    "workflow": "workspace:*",
    "xss": "catalog:",
    "zod": "catalog:",
    "zod-class": "0.0.16",
    "@aura/permissions": "workspace:*"
  }
}
