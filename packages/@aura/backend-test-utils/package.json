{
  "name": "@aura/backend-test-utils",
  "version": "0.22.0",
  "scripts": {
    "clean": "rimraf dist .turbo",
    "dev": "pnpm watch",
    "typecheck": "tsc --noEmit",
    "build": "tsc -p tsconfig.build.json",
    "format": "biome format --write .",
    "format:check": "biome ci .",
    "lint": "eslint . --quiet",
    "lint:fix": "eslint . --fix",
    "watch": "tsc -p tsconfig.build.json --watch",
    "test": "echo \"WARNING: no test specified\" && exit 0",
    "test:dev": "echo \"WARNING: no test specified\" && exit 0"
  },
  "main": "dist/index.js",
  "module": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@aura/backend-common": "workspace:^",
    "@aura/config": "workspace:^",
    "@aura/constants": "workspace:^",
    "@aura/db": "workspace:^",
    "@aura/di": "workspace:^",
    "@aura/permissions": "workspace:^",
    "@aura/typeorm": "catalog:",
    "jest-mock-extended": "^3.0.4",
    "workflow": "workspace:^",
    "reflect-metadata": "catalog:",
    "uuid": "catalog:"
  },
  "devDependencies": {
    "@aura/typescript-config": "workspace:*"
  }
}
