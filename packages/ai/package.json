{
	"name": "@aura/ai",
	"version": "1.0.0",
	"description": "AI functionality of AURA",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"clean": "rimraf dist .turbo",
		"dev": "pnpm watch",
		"typecheck": "tsc --noEmit",
		"build": "tsc -p tsconfig.build.json",
		"format": "biome format --write .",
		"format:check": "biome ci .",
		"lint": "eslint . --quiet",
		"lint:fix": "eslint . --fix",
		"watch": "tsc -p tsconfig.build.json --watch",
		"test": "jest",
		"test:dev": "jest --watch"
	},
	"files": [
		"dist/**/*"
	],
	"dependencies": {
		"@aura/utils": "workspace:*",
		"@aura/types": "workspace:*",
		"@aura/constants": "workspace:*",
		"@aura/errors": "workspace:*",
		"@anthropic-ai/sdk": "^0.27.0",
		"@google/generative-ai": "^0.21.0",
		"amqplib": "^0.10.0",
		"bullmq": "^4.18.3",
		"langchain": "^0.1.0",
		"openai": "^4.104.0",
		"pinecone-client": "^1.0.0",
		"weaviate-client": "^3.9.0",
		"better-sqlite3": "^11.0.0"
	},
	"devDependencies": {
		"@aura/typescript-config": "workspace:*",
		"@types/better-sqlite3": "^7.6.11",
		"@types/node": "*",
		"jest": "catalog:",
		"ts-jest": "catalog:",
		"typescript": "catalog:"
	},
	"license": "See LICENSE.md file in the root of the repository"
}
